{
  "name": "onetimesecret-api",
  "version": "2.1.1",
  "description": "node.js module for the OneTimeSecret API",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/tgamauf/onetimesecret-api.git"
  },
  "keywords": [
    "onetimesecret",
    "api",
    "async",
    "promise"
  ],
  "author": {
    "name": "Thomas Gamauf"
  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "dependencies": {
    "buffer": "^6.0.3",
    "cross-fetch": "^3.1.5",
    "node-fetch": ">=3.3.0"
  },
  "devDependencies": {
    "@types/istanbul-lib-report": "^3.0.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^3.0.2",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18.*"
  },
  "bugs": {
    "url": "https://github.com/tgamauf/onetimesecret-api/issues"
  },
  "homepage": "https://github.com/tgamauf/onetimesecret-api",
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    }
  }
}
