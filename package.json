{
  "name": "onetimesecret-api",
  "version": "2.1.0",
  "description": "node.js module for the OneTimeSecret API",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/tgamauf/onetimesecret-api.git"
  },
  "keywords": [
    "onetimesecret",
    "api",
    "async",
    "promise"
  ],
  "author": {
    "name": "Thomas Gamauf"
  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "dependencies": {
    "buffer": "^6.0.3",
    "cross-fetch": "^3.1.5",
    "node-fetch": ">=3.2.10"
  },
  "devDependencies": {
    "@types/istanbul-lib-report": "^3.0.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^3.0.2",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.1",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": "12.* || 14.x || >= 16.*"
  },
  "bugs": {
    "url": "https://github.com/tgamauf/onetimesecret-api/issues"
  },
  "homepage": "https://github.com/tgamauf/onetimesecret-api",
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    }
  }
}
